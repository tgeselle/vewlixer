				<SECTION name="CPU" required="false">
					<SECTIONTYPE>Global</SECTIONTYPE>
					<KEYS>
						<KEY name="CPU" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>CPU to use. You should only set this if you want to change whatever CPU you are using from the quick start mode you selected, like for example running an A4000 with a 68060 CPU.</DESCRIPTION>
							<VALUES>
								<VALUE>68000</VALUE>
								<VALUE>68010</VALUE>
								<VALUE>68020</VALUE>
								<VALUE>68030</VALUE>
								<VALUE>68040</VALUE>
								<VALUE>68060</VALUE>
							</VALUES>
						</KEY>
						<KEY name="CpuSpeed" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>CPU Emulation Speed. Real means "Approximate A500/A1200 or cycle-exact". Max is Fastest possible emulation. Default is real.</DESCRIPTION>
							<VALUES>
								<VALUE>real</VALUE>
								<VALUE>max</VALUE>
							</VALUES>
						</KEY>	
						<KEY name="CpuCycleExact" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Set to true if you want to enable CPU Cycle Exact.</DESCRIPTION>
						</KEY>	
						<KEY name="CpuCompatible" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>"More Compatible". Set to true if you want to use CPU Compatible mode (Prefetch Emulation), this should normally be disabled.</DESCRIPTION>
						</KEY>	
						<KEY name="MMU" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Enables the MMU. Requires a 68030 CPU or better. Default is false.</DESCRIPTION>
						</KEY>
						<KEY name="DisableUnimplementedCPU" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Emulate CPU unimplemented instructions. Requires a 68060 CPU. Default, when a 68060 CPU is set, is false (enabled).</DESCRIPTION>
						</KEY>	
						<KEY name="FPU" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>FPU to use. Requires a 68020 or 68030 CPU set. Only some rare demos or later 3D games might require a FPU so normally you won't need to change this setting. Default is no FPU.</DESCRIPTION>
							<VALUES>
								<VALUE>68881</VALUE>
								<VALUE>68882</VALUE>
								<VALUE description="CPU Internal">internal</VALUE>
							</VALUES>
						</KEY>					
						<KEY name="MoreCompatibleFPU" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Enables slower but more compatible FPU. Requires a FPU set. Default is false.</DESCRIPTION>
						</KEY>	
						<KEY name="DisableUnimplementedFPU" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Emulate FPU unimplemented instructions. Requires a FPU set. Default is true (disabled).</DESCRIPTION>
						</KEY>									
						<KEY name="CacheSize" required="false" nullable="false">
							<KEYTYPE>Integer</KEYTYPE>
							<DESCRIPTION>Enable JIT by defining this to anything other then 0. The value is in KB, so to enable JIT with 8MB cache, type 8192. Requires at least a 68020 CPU and will override any Cycle exact modes.</DESCRIPTION>
						</KEY>					
						<KEY name="24-BitAddressing" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>If you want to use Z3 memory, you need to set this to False. Default is true.</DESCRIPTION>
						</KEY>	
					</KEYS>
				</SECTION>	
				<SECTION name="Chipset" required="false">
					<SECTIONTYPE>Global</SECTIONTYPE>	
					<KEYS>						
						<KEY name="CycleExact" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Chipset Cycle-exact. This sets the entire system to cycle-exact so set to true if you want to enable global Cycle Exact. This will be automatically disabled if either CpuCycleExact or BlitterCycleExact in the CPU tab is set.</DESCRIPTION>
						</KEY>					
						<KEY name="ImmediateBlitter" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Set to true if you want to enable Immediate Blittler. This is mostly useful for some WHDLoad games that will show some random graphics glitches.</DESCRIPTION>
						</KEY>
						<KEY name="BlitterCycleExact" required="false" nullable="false">
							<KEYTYPE>Boolean</KEYTYPE>
							<DESCRIPTION>Set to true if you want to enable Blitter Cycle Exact.</DESCRIPTION>
						</KEY>							
						<KEY name="CollisionLevel" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>Set the desired collision level, very few games require changing this setting</DESCRIPTION>
							<VALUES>
								<VALUE description="None">none</VALUE>
								<VALUE description="Sprites Only">sprites</VALUE>
								<VALUE description="Sprites and Sprites vs. Playfield">playfields</VALUE>
								<VALUE description="Full">full</VALUE>
							</VALUES>
						</KEY>
					</KEYS>
				</SECTION>
				<SECTION name="RAM" required="false">
					<SECTIONTYPE>Global</SECTIONTYPE>	
					<KEYS>	
						<KEY name="ChipMemory" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>How much Chip RAM you want to use</DESCRIPTION>
							<VALUES>
								<VALUE description="256 KB">0</VALUE>
								<VALUE description="512 KB">1</VALUE>
								<VALUE description="1 MB">2</VALUE>
								<VALUE description="1.5 MB">3</VALUE>
								<VALUE description="2 MB">4</VALUE>
								<VALUE description="4 MB">5</VALUE>
								<VALUE description="8 MB">6</VALUE>
							</VALUES>
						</KEY>
						<KEY name="FastMemory" required="false" nullable="false">
							<KEYTYPE>String</KEYTYPE>
							<DESCRIPTION>How much Fast RAM you want to use</DESCRIPTION>
							<VALUES>
								<VALUE description="None">0</VALUE>
								<VALUE description="1 MB">1</VALUE>
								<VALUE description="2 MB">2</VALUE>
								<VALUE description="4 MB">4</VALUE>
								<VALUE description="8 MB">8</VALUE>
							</VALUES>
						</KEY>
