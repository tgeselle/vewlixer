								<VALUE description="A3000 - 3.1 ROM, 2MB Chip + 8MB Fast">a3000,2</VALUE>
								<VALUE description="A4000 - 68030, 3.1 ROM, 2MB Chip + 8MB Fast">a4000,0</VALUE>
								<VALUE description="A4000 - 68040, 3.1 ROM, 2MB Chip + 8MB Fast">a4000,1</VALUE>
							</VALUES>
						</KEY>
